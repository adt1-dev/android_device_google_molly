/sys/kernel/pmic                                   u:object_r:sysfs_pmic:s0
/sys/devices/soc[0-9]+(/.*)?                       u:object_r:sysfs_soc:s0
/sys/devices/virtual/gpio(/.*)?                    u:object_r:sysfs_gpio:s0

